<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample.query.Sample1">
	
	<select id="getData" resultType="java.util.HashMap">
		SELECT * FROM contact
	</select>
	
	<insert id="insertData" parameterType="java.util.HashMap" >
		INSERT INTO contact(subject,phone,email,question) VALUES(#{subject},#{phone},#{email},#{question})
	</insert>
	
</mapper>